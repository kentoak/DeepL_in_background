{
	"manifest_version": 2,
	"name": "DeepLBack",
	"version" : "1.0.0",
	"description": "バックグラウンドでDeepLの翻訳をしてくれる",
	"background" : {
		"scripts": ["deepl_background.js"]
	},
	"permissions": [
        "storage","unlimitedStorage","contextMenus","tabs"],
    "commands": {
		"_execute_browser_action": {
		  "suggested_key": {
			"default": "Alt+D",
			"windows": "Alt+D",
			"mac": "Alt+D"
		  },
		  "description": "Popup deepL!!!!"
        }
	},
	"icons": {
		"16" : "images/icon16.png",
		"32" : "images/icon32.png",
		"64" : "images/icon64.png",
		"128" : "images/icon128.png"
	}
}
